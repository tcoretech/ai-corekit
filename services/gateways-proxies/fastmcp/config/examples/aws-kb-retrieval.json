{
  "aws-kb-retrieval": {
    "enabled": false,
    "description": "AWS Knowledge Base Retrieval",
    "type": "external",
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-aws-kb-retrieval"
    ],
    "env": {
      "AWS_ACCESS_KEY_ID": "${AWS_ACCESS_KEY_ID}",
      "AWS_SECRET_ACCESS_KEY": "${AWS_SECRET_ACCESS_KEY}",
      "AWS_REGION": "${AWS_REGION}"
    }
  }
}
