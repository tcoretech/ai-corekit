# OpenClaw Configuration

# Secure token for gateway access (will be generated)
OPENCLAW_GATEWAY_TOKEN=

# Port for the gateway
OPENCLAW_GATEWAY_PORT=18789

# Port for the bridge
OPENCLAW_BRIDGE_PORT=18790

# Bind address (lan = accessible on network, local = localhost only)
OPENCLAW_GATEWAY_BIND=lan

# Keys for Claude AI integration (Optional/Required depending on usage)
CLAUDE_AI_SESSION_KEY=
CLAUDE_WEB_SESSION_KEY=
CLAUDE_WEB_COOKIE=

# Twilio (Optional)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_FROM=
